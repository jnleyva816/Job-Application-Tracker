<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Sample suppression entries - customize based on your needs -->
    
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes><![CDATA[
        Spring Boot starter modules are commonly flagged but are maintained by the Spring team
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Suppress test dependencies from production scanning -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not part of production deployment
        ]]></notes>
        <gav regex="true">.*:.*:.*:test</gav>
    </suppress>
    
    <!-- Example: Suppress specific CVE for a specific dependency version -->
